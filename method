package day7;
import java.util.Scanner;
public class method {
    public int add(int a, int b) {
        int sum = a + b;
        return sum;
    }
    public static void main(String[] args) {
        method obj = new method(); 
        Scanner sc = new Scanner(System.in);
        int num1 = sc.nextInt();
        int num2 = sc.nextInt();
        int result = obj.add(num1, num2);
        System.out.println("Sum: " + result);
    }
}
